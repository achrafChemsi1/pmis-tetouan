{
  "name": "pmis-tetouan-backend",
  "version": "1.0.0",
  "description": "PMIS Tétouan Backend API - Project Management Information System for Prefecture of Tétouan",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "seed": "node src/database/seeders.js",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\""
